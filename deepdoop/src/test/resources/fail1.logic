
P(x) -> string(x).
P("hello").

.comp Foo {
	P1(x) <- P(x).
	P1(x) -> string(x).
}
.comp Bar {
	P2(x) <- P1@past(x).
}
.comp Baz {
	P3(x) <- P2@past(x).
}

.init S1 = Foo
.init S2 = Bar
.init S3 = Baz

.propagate S1 { * } to S2
.propagate S2 { * } to S3
.propagate S3 { * } to S1
.propagate S3 { P3 } to .global
