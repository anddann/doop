P(x) -> string(x).

.comp Foo {
	P1(x) <- P(x).
}

.cmd Bar {
	lang:cmd:DIR[] = "../dir1".
	lang:cmd:EVAL[] = "dir2/run.sh -flag1".
	lang:cmd:export(`P1).
	lang:cmd:import(`A).
	lang:cmd:import(`B).
	A(x, y) -> string(x), string(y).
	B(x) -> int(x).
	B(1).
}

.init S1 = Foo
.init S2 = Bar

.propagate S1 { * } to S2
.propagate S2 { * } to .global
