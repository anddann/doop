P(x) -> string(x).

component Foo {
	P1(x) <- P(x).
}

command Bar {
	lang:cmd:DIR[] = "../dir1".
	lang:cmd:EVAL[] = "dir2/run.sh -flag1".
	lang:cmd:export(`P1).
	lang:cmd:import(`A).
	lang:cmd:import(`B).
	A(x, y) -> string(x), string(y).
	B(x) -> int(x).
	B(1).
}

S1 = new Foo
S2 = new Bar

propagate S1 { * } to S2
propagate S2 { * } to .global
