P(x) -> string(x).
Q(x, y) -> string(x), int(y).

Q(x, y) <- P(x), y = 10.

component Foo {
	P1(x) <- P(x).
	P1(x) -> string(x).
}

component Baz {
	Final(x, y) <- A@past(x,_), B@past(y).
}

command Bar {
	lang:cmd:DIR[] = "../dir1".
	lang:cmd:EVAL[] = "dir2/run.sh -flag1".
	lang:cmd:export(`P1).
	lang:cmd:import(`A).
	lang:cmd:import(`B).
	A(x, y) -> string(x), string(y).
	B(x) -> int(x).
	C(x) -> string(x).
}

S1 = new Foo
S2 = new Bar
S3 = new Baz

propagate S1 { * } to S2
propagate S2 { * } to S3
propagate S3 { * } to .global
