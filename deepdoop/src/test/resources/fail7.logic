P(x) -> string(x).

component Foo {
	P1(x) <- P(x).
}

command Bar {
	lang:cmd:DIR[] = "../dir1".
	lang:cmd:EVAL[] = "dir2/run.sh -flag1".
	lang:cmd:export(`P1).
	lang:cmd:import(`A).
	lang:cmd:import(`B).
	A(x, y) -> string(x), string(y).
	lang:compiler:warning:SPECIFIC_STARRED_EDGE_IN_SAFETY_GRAPH_CYCLE[] = false.
}

S1 = new Foo
S2 = new Bar

propagate S1 { * } to S2
propagate S2 { * } .global
